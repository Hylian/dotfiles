! Merge with xrdb -merge ~/.Xresources

URxvt.font:      xft:Input Mono:pixelsize=14

URxvt.scrollBar:        false
! URxvt.borderLess:       true
URxvt.savelines:        4000

URxvt.perl-lib: ~/.urxvt/ext/
URxvt.perl-ext-common: default,matcher,keyboard-select

URxvt.matcher.button: 1
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&amp;@#-]*[\\w/-]
URxvt.url-launcher: /usr/local/bin/qutebrowser

URxvt.keysym.M-v: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.C-C: eval:selection_to_clipboard
URxvt.keysym.C-V: eval:paste_clipboard
URxvt.keysym.M-b: matcher:last

URxvt.iso14755:    false
URxvt.iso14755_52: false

URxvt.background: #2a2a2a 
URxvt.foreground: #d0d0d0 
URxvt.color0: #4e4e4e
URxvt.color1: #d68787 
URxvt.color2: #5f865f
URxvt.color3: #d8af5f
URxvt.color4: #85add4
URxvt.color5: #d7afaf
URxvt.color6: #87afaf
URxvt.color7: #d0d0d0
URxvt.color8: #626262
URxvt.color9: #d75f87
URxvt.color10: #87af87
URxvt.color11: #ffd787 
URxvt.color12: #add4fb
URxvt.color13: #ffafaf
URxvt.color14: #87d7d7
URxvt.color15: #e4e4e4
URxvt.cursorColor: #d0d0d0
URxvt.cursorColor2: #3a3a3a
URxvt.colorBD: #e4e4e4
Xcursor.size: 16
Xft.antialias: 1
rxvt.print-pipe: "cat > /dev/null"
urxvt.print-pipe: "cat > /dev/null"

Xcursor.theme: captain-frank-cursors

Xft.dpi: 150
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
